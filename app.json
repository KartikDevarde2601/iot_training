{
  "name": "iot_training",
  "displayName": "iot_training",
  "expo": {
    "name": "iot_training",
    "slug": "iot_training",
    "scheme": "iot_training",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/app-icon-all.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/images/app-icon-android-legacy.png",
      "package": "com.iottraining",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-android-adaptive-foreground.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "allowBackup": false,
      "splash": {
        "backgroundColor": "#fff"
      }
    },
    "ios": {
      "icon": "./assets/images/app-icon-ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.iottraining",
      "splash": {
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "bundler": "metro",
      "splash": {
        "backgroundColor": "#fff"
      }
    },
    "plugins": [
      [
        "react-native-ble-plx"
      ],
      "expo-localization",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon-android-adaptive-foreground.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#191015"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "backgroundColor": "#fff"
    }
  },
  "ignite": {
    "version": "10.3.0"
  }
}
